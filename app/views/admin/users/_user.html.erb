<tr>
  <td><%= create_index params[:page], user_counter, params[:limit] %></td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td>
    <% if can? :destroy, user %>
      <%= link_to "", admin_user_path(user), class: "fa fa-trash",
        method: :delete, data: {confirm: t("confirm.are_you_sure")} %>
    <% end %>
  </td>
</tr>
