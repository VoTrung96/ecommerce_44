<footer class="sticky-footer">
  <div class="container">
    <div class="text-center">
      <small><%= t "admin.footer.copy_right" %></small>
    </div>
  </div>
</footer>
<a class="scroll-to-top rounded" href="#page-top">
  <i class="fa fa-angle-up"></i>
</a>
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel"><%= t "admin.footer.confirm" %></h5>
        <button class="close" type="button" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">Ã—</span>
        </button>
      </div>
      <div class="modal-body"><%= t "admin.footer.intruction" %></div>
      <div class="modal-footer">
        <button class="btn btn-secondary" type="button" data-dismiss="modal"><%= t "admin.footer.cancle" %></button>
        <%= link_to t("admin.dashboard.logout"), destroy_user_session_path, class: "btn btn-primary", method: :delete %>
      </div>
    </div>
  </div>
</div>
