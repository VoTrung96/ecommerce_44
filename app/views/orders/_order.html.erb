<tr>
  <td><%= l order.created_at, format: :long %></td>
  <td>
    <%= link_to t(".view_detail"), order_path(order), class: "ps-product-link" %>
  </td>
  <td><%= order.status %></td>
  <td>
    <% if order.pendding? %>
      <%= link_to "", order_path(order), class: "ps-remove", method: :patch %>
    <% end %>
  </td>
</tr>
